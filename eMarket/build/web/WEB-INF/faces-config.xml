<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/catalog.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>fromcatalog</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>tocreate-product2</from-outcome>
            <to-view-id>/create-product.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>toshoppingcart2</from-outcome>
            <to-view-id>/ShoppingCart.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>tocatalog</from-outcome>
            <to-view-id>/catalog.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>toshoppingcart</from-outcome>
            <to-view-id>/ShoppingCart.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>tocreate-product</from-outcome>
            <to-view-id>/create-product.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/ShoppingCart.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>fromshoppingcart</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/create-product.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>fromcreate-product</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>fromcreate-product2</from-outcome>
            <to-view-id>/catalog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
